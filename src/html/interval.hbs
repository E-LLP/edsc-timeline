<g class="{{ns}}-date-label" transform="translate({{x0}}, 0)">
  <rect x="0" y="0" width="{{width}}" height="2000"></rect>
  <line x1="0" y1="-1000" x2="0" y2="1000"
        class="{{ns}}-line{{#if subText}} {{ns}}-interval-start{{/if}}"></line>
  <circle class="{{ns}}-tick-crossing" r="6" cx="{{width}}"></circle>
  <text x="5" y="20" class="{{ns}}-axis-label {{ns}}-axis-super-label">{{text}}</text>
  {{#if subText}}
    <text x="5" y="34" class="{{ns}}-axis-label {{ns}}-axis-sub-label">{{subText}}</text>
  {{/if}}
</g>
